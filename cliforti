config webfilter profile
    edit "default"
        set comment "Default web filtering."
        set replacemsg-group ''
        set inspection-mode flow-based
        unset options
        set https-replacemsg enable
        unset ovrd-perm
        set post-action normal
        config override
            set ovrd-cookie deny
            set ovrd-scope user
            set profile-type list
            set ovrd-dur-mode constant
            set ovrd-dur 15m
        end
        config web
            set bword-threshold 10
            unset bword-table
            unset urlfilter-table
            unset content-header-list
            set blacklist disable
            unset whitelist
            unset safe-search
            set log-search disable
        end
        set youtube-channel-status disable
        config ftgd-wf
            unset options
            set exempt-quota 17
            unset ovrd
            config filters
